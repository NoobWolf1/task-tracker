{
  "name": "task-tracker",
  "version": "1.0.0",
  "description": "A simple CLI bassed task tracker tool.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc index.ts",
    "start": "node dist/index.js",
    "dev": "npm run build && npm start",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "format": "prettier --check 'src/**/*.{ts,js,json,md}'",
    "lint:fix": "eslint 'src/**/*.{ts,js}' --fix",
    "format:fix": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "fix": "npm run lint:fix && npm run format:fix",
    "pre-commit": "npm run lint && npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoobWolf1/task-tracker.git"
  },
  "keywords": [
    "Node.js"
  ],
  "author": "Malay Shukla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NoobWolf1/task-tracker/issues"
  },
  "homepage": "https://github.com/NoobWolf1/task-tracker#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  }
}
